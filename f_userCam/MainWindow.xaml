<Window x:Class="f_userCam.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:f_userCam"
        mc:Ignorable="d"
        Height="600" Width="1000">
    <Grid>
        <Image Name="camera" />
        <Canvas Name="canvas" />

        <StatusBar Name="Output_Sequence" HorizontalAlignment="Left" Height="41" VerticalAlignment="Bottom" Width="1000">
            <Label Name="Output_Label" Content="Lab" Width="1892" FontSize="16" Height="31"/>
        </StatusBar>
    </Grid>
</Window>
